{
  "name": "zero-zero",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Zero Zero - A sustainability app that helps users save money, reduce their carbon footprint, and improve well-being through real-time data and AI-powered insights",
  "engines": {
    "node": "18.20.8",
    "npm": "10.8.2"
  },
  "scripts": {
    "dev": "vite",
    "build": "npx tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "clean": "rm -rf node_modules/.cache",
    "clean:cache": "npm cache clean --force && rm -rf node_modules/.cache",
    "validate": "node validate-env.js",
    "test-env": "node validate-env.js",
    "postbuild": "node verify-build.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.52.1",
    "framer-motion": "^10.16.16",
    "openai": "^5.10.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.7.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.8.3",
    "vite": "^7.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zerozero/zero-zero-app.git"
  },
  "keywords": [
    "sustainability",
    "carbon-footprint",
    "environment",
    "react",
    "typescript",
    "vite",
    "netlify"
  ],
  "author": "Zero Zero Team",
  "license": "MIT",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
